{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product_name": {
      "type": "string"
    },
    "banner_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "product_heading": {
      "type": "string"
    },
    "product_url": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "product_big_view": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "home_view": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "thumnail_view": {
      "type": "boolean",
      "default": true,
      "required": false
    },
    "product_category": {
      "type": "enumeration",
      "enum": [
        "Products",
        "Photography & Retouching",
        "Printing & Fabrication"
      ]
    },
    "options_images": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::options-image.options-image",
      "mappedBy": "product"
    }
  }
}
