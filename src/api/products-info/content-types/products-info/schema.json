{
  "kind": "collectionType",
  "collectionName": "products_infos",
  "info": {
    "singularName": "products-info",
    "pluralName": "products-infos",
    "displayName": "Products Info"
  },
  "options": {
    "draftAndPublish": true,
    "populateCreatorFields": true
  },
  "pluginOptions": {},
  "attributes": {
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category",
      "inversedBy": "products_info"
    },
    "descriptions": {
      "type": "text"
    },
    "slider_image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "banner_image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "products_url": {
      "type": "string"
    }
  }
}
